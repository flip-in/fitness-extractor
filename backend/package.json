{
  "name": "fitness-extractor-backend",
  "version": "1.0.0",
  "description": "Backend API server for Fitness Extractor - receives HealthKit data and serves dashboard",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "test": "echo \"Tests to be implemented\" && exit 0"
  },
  "keywords": [
    "fitness",
    "healthkit",
    "api",
    "express",
    "postgresql"
  ],
  "author": "William Price",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/cors": "^2.8.17",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
